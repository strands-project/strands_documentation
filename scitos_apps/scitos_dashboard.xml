<?xml version="1.0"?>
<package>
  <name>scitos_dashboard</name>
  <version>0.1.1</version>
  <description>The scitos_dashboard package providing and rqt interface for the scitos robot.</description>

  <maintainer email="cburbridge@gmail.com">chris burbridge</maintainer>
  <author email="cburbridge@gmail.com">chris burbridge</author>

  <license>GPL</license>

  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>roslib</build_depend>
  <build_depend>rospy</build_depend>
  <build_depend>rqt_gui</build_depend>
  <build_depend>rqt_robot_dashboard</build_depend>
  <build_depend>scitos_msgs</build_depend>
  <build_depend>std_msgs</build_depend>
  <run_depend>roslib</run_depend>
  <run_depend>rospy</run_depend>
  <run_depend>rqt_gui</run_depend>
  <run_depend>rqt_robot_dashboard</run_depend>
  <run_depend>scitos_msgs</run_depend>
  <run_depend>std_msgs</run_depend>



  <export>
    <rqt_gui plugin="${prefix}/plugin.xml"/>
  </export>
</package>
