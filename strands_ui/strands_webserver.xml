  <maintainer email="c.j.c.burbridge@cs.bham.ac.uk">Chris Burbridge</maintainer> 

  <license>MIT</license>

  <author email="n.a.hawes@cs.bham.ac.uk">Nick Hawes</author>
  <author email="c.j.c.burbridge@cs.bham.ac.uk">Chris Burbridge</author> 

